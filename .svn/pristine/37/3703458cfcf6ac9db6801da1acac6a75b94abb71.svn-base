    .wrap img {
        position: absolute;
        bottom: 0;
        padding-bottom: 10px;
    }

    .wrap img:nth-of-type(1) {
        left: 0;
        height: 80px;
        width: 80px;
    }

    .wrap img:nth-of-type(2) {
        right: 0;
        height: 80px;
        width: 90px;
        padding-right: 5px;
    }

  div.xhelper {
    min-height: 400px;
    width: 400px;
    border-style: inset;
    border-width: 2px;
    border-color: black;
    -webkit-transform: perspective( 300px ) rotateY( 25deg );
    -moz-transform: perspective( 300px ) rotateY( 25deg );
    -ms-transform: perspective( 300px ) rotateY( 25deg );
    -o-transform: perspective( 300px ) rotateY( 25deg );
    transform: perspective( 300px ) rotateY( 25deg );
   }
  
  @media only screen and (max-width:479px) {
      div.xhelper{ width: 90%;height: 150%;}
      .wrap img { width: 90%;}
      .fontA { width: 90%;}
      .fontB { width: 90%;}
      .fontC { width: 90%;}
      .fontD { width: 90%;}
  }
  
  
