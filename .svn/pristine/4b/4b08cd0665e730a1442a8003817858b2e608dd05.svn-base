            width: 300px;
            border-style: solid;
            border-width: 2px;
            border-color: royalblue;
            -ms-transform: skewY(-25deg); /* IE 9 */
            -webkit-transform: skewY(-25deg); /* Safari */
            transform: skewY(-25deg); /* Standard syntax */
            }

        .fontA {
            color: maroon;

        }
        .fontB {
            color: black;
        }
